{
    "name": "freitasmurillo/rumble",
    "description": "A dynamodb migration tool",
    "type": "library",
    "minimum-stability": "stable",
    "license": "MIT",
    "keywords": [
        "dynamodb",
        "migration",
        "php"
    ],
    "authors": [
        {
            "name": "Lawrence Enehizena",
            "email": "lawstands@gmail.com"
        },
        {
            "name": "Murillo Brendel Freitas",
            "email": "murillobrendelfreitas@gmail.com"
        }
    ],
    "require": {
        "symfony/console": "^3.2",
        "aws/aws-sdk-php": "3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "bin": ["bin/rumble"],
    "autoload": {
        "psr-4": {
            "Rumble\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rumble\\Tests\\": "tests/"
        }
    }
}
